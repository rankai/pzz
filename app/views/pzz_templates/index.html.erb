<h1>Listing pzz_templates</h1>

<table>
  <thead>
    <tr>
      <th>Template key</th>
      <th>Template value</th>
      <th>Template type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pzz_templates.each do |pzz_template| %>
      <tr>
        <td><%= pzz_template.template_key %></td>
        <td><%= pzz_template.template_value %></td>
        <td><%= pzz_template.template_type %></td>
        <td><%= link_to 'Show', pzz_template %></td>
        <td><%= link_to 'Edit', edit_pzz_template_path(pzz_template) %></td>
        <td><%= link_to 'Destroy', pzz_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pzz template', new_pzz_template_path %>
