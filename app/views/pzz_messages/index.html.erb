<h1>Listing pzz_messages</h1>

<table>
  <thead>
    <tr>
      <th>From user</th>
      <th>To user</th>
      <th>From user nickname</th>
      <th>Message subject</th>
      <th>Message content</th>
      <th>Message folder</th>
      <th>Message related</th>
      <th>Message status</th>
      <th>Message type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pzz_messages.each do |pzz_message| %>
      <tr>
        <td><%= pzz_message.from_user_id %></td>
        <td><%= pzz_message.to_user_id %></td>
        <td><%= pzz_message.from_user_nickname %></td>
        <td><%= pzz_message.message_subject %></td>
        <td><%= pzz_message.message_content %></td>
        <td><%= pzz_message.message_folder %></td>
        <td><%= pzz_message.message_related_id %></td>
        <td><%= pzz_message.message_status %></td>
        <td><%= pzz_message.message_type %></td>
        <td><%= link_to 'Show', pzz_message %></td>
        <td><%= link_to 'Edit', edit_pzz_message_path(pzz_message) %></td>
        <td><%= link_to 'Destroy', pzz_message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pzz message', new_pzz_message_path %>
