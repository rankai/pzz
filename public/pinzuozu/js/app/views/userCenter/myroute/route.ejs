<div class="page-list-bg p10 mt20 frame-border">
  <div class="mt20 ml20">
    <h4>我的拼车线路</h4>
  </div>
  <div class="user-cr frame-border">

    <table class="table table-striped">
      <thead>
        <tr>
          <th>出发日期</th>
          <th>线路</th>
          <th>剩余座位</th>
          <th>操作</th>
        </tr>
      </thead>
      <tbody>
        <% for(var i=0;i<lines.length;i++){ %>
        <tr>
          <td><i class="fa fa-lg pr15 <%= lines[i].user_type=='passenger'?'fa-user cr3b9':'fa-car cr999' %>"></i>
            <%= lines[i].line_depart_datetime %>
          </td>

          <td>
            <a href="">
              <%=lines[i].line_depart_city %>:<%=lines[i].line_depart_address %> 
              ->
              <%=lines[i].line_dest_city %>:<%=lines[i].line_dest_address %>
            </a>
          </td>
          <td>
            <input class="form-control" type="text" value="<%=lines[i].line_participants_available %>" id="line_participants_available<%=lines[i].id%>" />
            <!--
            <select class="form-control">
              <option>1</option>
              <option>2</option>
              <option>3</option>
              <option>4</option>
              <option>5</option>
            </select>
            -->
          </td>
          <td>
            <a class="line_participants_available" href="javascript:void(0)" id="<%=lines[i].id%>">保存</a>
            <a class="ml10 cancelLine" href="javascript:void(0)" id="cancel_<%=lines[i].id%>">取消</a>
          </td>
        </tr>
        <% } %>
        
      </tbody>
    </table>

  </div>
</div>